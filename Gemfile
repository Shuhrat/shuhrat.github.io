source 'https://rubygems.org'

require 'json'
require 'open-uri'
versions = JSON.parse(open('https://pages.github.com/versions.json').read)

gem 'jemoji'
gem 'jekyll-gist'
gem 'jekyll-feed'
gem 'jekyll-compose'
gem 'jekyll-sitemap'
gem 'jekyll-paginate'
gem 'jekyll-mentions'
gem 'jekyll-redirect-from'

gem 'github-pages', versions['github-pages']

